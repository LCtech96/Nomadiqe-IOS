{
  "expo": {
    "name": "Nomadiqe",
    "slug": "nomadiqe-ios",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4F46E5"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nomadiqe.app",
      "infoPlist": {
        "NSCameraUsageDescription": "Nomadiqe needs access to your camera to upload photos and videos",
        "NSPhotoLibraryUsageDescription": "Nomadiqe needs access to your photo library to upload photos and videos",
        "NSLocationWhenInUseUsageDescription": "Nomadiqe needs your location to show nearby properties",
        "UIBackgroundModes": [
          "remote-notification",
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4F46E5"
      },
      "package": "com.nomadiqe.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Nomadiqe needs access to your photos to let you share them."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#4F46E5"
        }
      ],
      "expo-localization"
    ],
    "scheme": "nomadiqe",
    "extra": {
      "eas": {
        "projectId": "f8760479-d3b7-4660-a96d-b5a081ba294d"
      }
    }
  }
}
